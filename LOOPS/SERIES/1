#include<stdio.h>

int main()
{
    int i,j,k=1,a;
 
    printf("How many lines do you want? :");
    scanf("%d",&a);
 
    for(i=1;i<a;i++)
    {
    	for(j=0;j<a-i;j++)
    	printf("*");
        for(j=1;j<=i;j++,k++)
        {
            printf("%d ",k);
        }
        
        
         printf("\n");
    }
	return 0;
}
